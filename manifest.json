{
  "name": "DevScreen",
  "description": "Designed for Web Developers with customized search engines and easy access to sites Developers use everyday",
  "manifest_version": 2,
  "version": "1.0.0",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ]
}